    "@headlessui/vue": "^1.7.17",
    "@vuelidate/core": "^2.0.3",
    "@vuelidate/validators": "^2.0.4",
    "@vueuse/core": "^10.7.1",
    "@vueuse/integrations": "^10.9.0",
    "apexcharts": "^3.45.2",
    "libphonenumber-js": "^1.10.53",
    "pinia": "^2.1.7",
